{% extends 'project/edit.base.html.twig' %}

{% block edit_page_title%}{{ 'project.edit.common'|trans }}{% endblock %}

{% block edit_page %}
    {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.icon) }}
        {{ form_row(form.description) }}
{% endblock %}

{% block box_footer %}
    <div class="pull-right">
        <button type="submit" class="btn btn-success">{{ 'Edit'|trans }}</button>
    </div>
    {{ form_end(form) }}
{% endblock %}