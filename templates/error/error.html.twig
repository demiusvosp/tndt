{% extends 'base_layout.html.twig' %}
{% import 'project/_macro.html.twig' as mProject %}
{% trans_default_domain "errors" %}
{% block title %}{{ parent() }} - {{ status_label }}{% endblock %}
{% block page_title%}{% endblock %}
{% block page_subtitle %}{% endblock %}

{% block breadcrumb %}
{% endblock %}

{% block page_content %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title error-title">{{ 'Error_page.reason'|trans }}: <span class="error-code">{{ status_code }}</span> - {{ status_label }}</h3>
        </div>
        <div class="panel-body">
            {{ 'Error_page.message'|trans }}: {{ status_description }}
        </div>
    </div>
{% endblock %}